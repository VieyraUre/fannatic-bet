section(class="casino")
    h2(class="casino__h2") Casino
    div(class="casino__layout")
        div(class="casino__layout--col1")
            div(class="casino__layout--grid ed-grid s-grid-4")
                -for(var i = 0; i < casinos.length ; i++)
                    -var casino = casinos[i]
                    div(class="casino__layout--card")
                        img(src=casino.img, alt=casino.alt)
                        h3(class="casino__layout--h3")= casino.title
        div(class="casino__layout--col2") 
            h3(class="casino__layout--title")  PROVIDERS
            div(class="ed-grid s-grid-2 casino__layout--imgs")
                -for(var i = 0; i < providers.length ; i++)
                    -var provider = providers[i]
                    img(src=provider.img, alt=provider.alt)
